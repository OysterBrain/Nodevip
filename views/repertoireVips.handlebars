

<p>Cliquez sur une lettre :
    {{#each lettres}}
        <a href="/repertoire/{{this.lettre}}">{{this.lettre}}</a>
    {{/each}}
</p>


{{#each infoStars}}
    <div class="photoRep">
        <a href="/repertoire/Info/{{this.id}}"><img   src="/images/vip/{{this.photo}}" width="100" height="125" ></a>
        <p><a href="/repertoire/Info/{{this.id}}">{{this.prenom}} {{this.nom}}</a></p>
    </div>
{{/each}}

