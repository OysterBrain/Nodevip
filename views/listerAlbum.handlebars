<h1> Album photo </h1>

{{#each allPhotos}}
    <div class="photoAlbum">
        <a href="/repertoire/Info/{{this.nom}}"><img   src="/images/vip/{{this.photo}}" width="100" height="125" ></a>
        <p><a href="/repertoire/Info/{{this.nom}}">{{this.prenom}} {{this.nom}}</a></p>
    </div>
{{/each}}
